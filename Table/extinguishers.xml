<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>laststampdate</name>
        <type>date</type>
    </column>
    <column>
        <name>manufacturedate</name>
        <type>date</type>
    </column>
    <column>
        <name>referencia</name>
        <type>character varying(30)</type>
        <null>NO</null>
    </column>
    <column>
        <name>serialnumber</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>extinguishers_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_extinguishers_variantes</name>
        <type>FOREIGN KEY (referencia) REFERENCES variantes (referencia) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>extinguishers_unique</name>
        <type>UNIQUE (serialnumber)</type>
    </constraint>
</table>